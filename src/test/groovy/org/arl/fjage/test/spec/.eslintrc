{
    "env": {
        "es6": true,
        "browser": true,
        "jasmine": true
    },
    "parserOptions": {
            "ecmaVersion": 2017,
            "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": 0,
        "indent": ["error", 2],
        "quotes":["error", "single"],
        "semi": ["error", "always"],
        "valid-jsdoc": ["error"]
    }
}
